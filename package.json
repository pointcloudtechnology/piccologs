{
    "name": "@pointcloudtechnology/piccologs",
    "version": "0.1.0",
    "description": "Simple CLI tool for generating changelogs; inspired by changesets",
    "keywords": [
        "changelogs"
    ],
    "author": "Point Cloud Technology GmbH",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/pointcloudtechnology/piccologs"
    },
    "type": "module",
    "module": "index.js",
    "exports": {
        ".": {
            "import": "./index.js"
        }
    },
    "files": [
        "bin",
        "src",
        "index.js"
    ],
    "bin": {
        "picco": "bin/picco.mjs"
    },
    "scripts": {
        "init": "npx picco init",
        "add": "npx picco add",
        "version": "npx picco version"
    },
    "dependencies": {
        "@clack/prompts": "0.7.0",
        "human-id": "4.1.1",
        "picocolors": "1.0.0"
    }
}
